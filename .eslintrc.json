{
  "rules": {
    "lines-between-class-members": "off",
    "class-methods-use-this":"off",
    "no-console": "off"
  },
  "env": {
    "es6": true,
    "node": true,
    "jest": true
  },
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "extends": [
    "eslint:recommended",
    "plugin:prettier/recommended",
    "airbnb"
  ], 
  "plugins": ["prettier"]
}
