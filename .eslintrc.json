{
  "env": {
    "browser": true,
    "commonjs": true,
    "es2021": true
  },
  "extends": "airbnb-base",
  "overrides": [],
  "parserOptions": {
    "ecmaVersion": "latest"
  },
  "rules": {
    "max-depth": ["error", 2],
    "max-lines-per-function": ["error", 12],
    "quotes": ["error", "double"]
  }
}
