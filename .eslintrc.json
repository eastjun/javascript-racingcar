{
  "rules": {
    "max-depth": ["error", 2],
    "max-lines": ["error", 10],
    "max-params": ["error", 3],
    "no-console": "off"
  },
  "env": {
    "es6": true,
    "node": true
  },
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "extends": ["eslint:recommended", "plugin:prettier/recommended"]
}
