{
  "rules": {
    "import/extensions": ["error", { "js": "ignorePackages" }],
    "import/order": [
      "error",
      {
      "alphabetize": {
      "order": "asc",
      "caseInsensitive": true
      }}]
  },
  "env": {
    "es6": true,
    "node": true,
    "jest": true
  },
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "plugins": ["node"],
  "extends": ["airbnb-base", "prettier"]
}
