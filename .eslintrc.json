{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": ["airbnb-base"],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
    "no-alert": "off",
    "max-depth": ["error", 2],
    "import/extensions": ["off"],
    "no-new": "off",
    "newline-before-return": 2,
    "class-methods-use-this": "off",
    "no-console": "warn",
    "operator-linebreak": "off"
  }
}
