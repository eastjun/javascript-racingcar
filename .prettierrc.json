{
  "singleQuote": true,
  "semi": true,
  "useTabs": false,
  "tabWidth": 2,
  "trailingComma": "all",
  "printWidth": 80,
  "bracketSpacing": true,
  "arrowParens": "always",
  "endOfLine": "auto",
  "maxDepth": 2,
  "max-lines-per-function": ["error", { "max": 10 }],
  "importOrder": ["^\\u0000", "^@\\w", "^[^.]"]
}
